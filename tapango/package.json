{
  "name": "tapango-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "description": "TAPANGO Mobile App - Cargo Logistics Platform",
  "scripts": {
    "ci:storybook:guard": "node scripts/check-storybook-runtime.mjs",
    "fonts:scan": "node scripts/scan-fonts.js",
    "start": "expo start",
    "start:dev": "expo start --dev-client",
    "start:tunnel": "expo start --tunnel",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build": "eas build",
    "build:ios": "eas build --platform ios",
    "submit": "eas submit",
    "test": "echo \"Tests are intentionally removed from this project\"",
    "test:unit": "jest --passWithNoTests",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --cache && node scripts/lint-rnw-shadows.mjs",
    "lint:ci": "node scripts/lint-rnw-shadows.mjs",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix --cache",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf node_modules .expo",
    "reinstall": "npm ci",
    "prebuild": "expo prebuild",
    "db:apply": "supabase db push",
    "db:login": "supabase login",
    "db:version": "supabase --version",
    "audit": "npm audit --audit-level=moderate",
    "deps:check": "expo doctor && npm outdated",
    "storybook": "storybook dev -p 6006 -c storybook",
    "storybook:build": "storybook build -c storybook",
    "test:ui": "playwright test",
    "test:ui:update": "playwright test --update-snapshots",
    "prepare": "husky"
  },
  "dependencies": {
    "@clerk/clerk-expo": "^2.14.30",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@lottiefiles/dotlottie-react": "^0.16.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "11.4.1",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@supabase/supabase-js": "^2.57.4",
    "@tamagui/config": "^1.132.23",
    "@tamagui/font-inter": "^1.132.23",
    "@tamagui/lucide-icons": "^1.132.23",
    "@tamagui/toast": "^1.132.23",
    "@tanstack/react-query": "^5.87.4",
    "expo": "^54.0.7",
    "expo-application": "^7.0.7",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "~15.0.7",
    "expo-camera": "~17.0.7",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.8",
    "expo-crypto": "~15.0.7",
    "expo-device": "~8.0.7",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-navigation-bar": "~5.0.8",
    "expo-notifications": "~0.32.11",
    "expo-print": "~15.0.7",
    "expo-router": "~6.0.4",
    "expo-secure-store": "~15.0.7",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.10",
    "expo-web-browser": "~15.0.7",
    "lottie-react-native": "^7.3.4",
    "onesignal-expo-plugin": "^1.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-onesignal": "^5.0.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "sentry-expo": "~7.0.0",
    "tamagui": "^1.132.23"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@babel/core": "^7.28.4",
    "@playwright/test": "^1.55.0",
    "@react-native/eslint-config": "0.76.0",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@tamagui/babel-plugin": "1.132.23",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.1.13",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "8.57.1",
    "eslint-config-expo": "10.0.0",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-react-hooks": "4.6.2",
    "eslint-plugin-react-native": "4.1.0",
    "expo-build-properties": "^1.0.8",
    "husky": "9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.11",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "react-test-renderer": "19.1.0",
    "rimraf": "5.0.5",
    "storybook": "8.6.14",
    "typescript": "5.9.2",
    "vite": "6.3.6",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "expo": {
    "plugins": [
      [
        "sentry-expo",
        {
          "project": "tapango-mobile"
        }
      ]
    ]
  },
  "overrides": {
    "semver": "^7.5.4",
    "@sentry/browser": "^7.119.1",
    "@sentry/react": "^7.119.1",
    "@sentry/react-native": ">=5.34.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "private": true
}
