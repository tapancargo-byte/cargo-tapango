# Expo environment variables (client-safe)
# NOTE: Do NOT place server credentials or org secrets here. These values are bundled into the app.

# OneSignal App ID (from OneSignal dashboard: Settings > Keys & IDs)
EXPO_PUBLIC_ONESIGNAL_APP_ID=

# Privacy Policy URL (shown in the SMS section)
EXPO_PUBLIC_PRIVACY_URL=https://www.privacypolicies.com/live/1485f8a9-f52f-4c73-a826-d2b51c4b4f23

# Push token registration URL (Supabase Edge Function)
# Using your project URL returned via MCP: https://ehlzbibwyqxowhwxpuoh.supabase.co
EXPO_PUBLIC_PUSH_REGISTER_URL=https://ehlzbibwyqxowhwxpuoh.supabase.co/functions/v1/register-push

# SMS sending URL (Supabase Edge Function)
EXPO_PUBLIC_SMS_SEND_URL=https://ehlzbibwyqxowhwxpuoh.supabase.co/functions/v1/send-sms

# Feature flags
# Gate SMS sends from the app (recommended: leave false in local dev)
EXPO_PUBLIC_ENABLE_BOOKING_SMS=false

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key

# Clerk Configuration
EXPO_PUBLIC_CLERK_PUBLISHABLE_your_clerk_publishable_key

# API Configuration
EXPO_PUBLIC_API_BASE_URL=http://localhost:3000

# Development Settings
EXPO_PUBLIC_ENVIRONMENT=development
